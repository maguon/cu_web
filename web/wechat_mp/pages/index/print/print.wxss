.count{
  width: 100%;
  height: 40px;
  background: white;
  display: flex;
  flex-direction: row;
}
.stepper { 
    width: 70px; 
    height: 26px; 
    border: 1px solid #ccc; 
    border-radius: 3px; 
    position: absolute;
    left: 15px;
    margin-top: 5px;
} 
.stepper text { 
    width: 19px; 
    line-height: 26px; 
    text-align: center; 
    float: left; 
} 
.stepper input { 
    width: 30px; 
    height: 26px; 
    float: left; 
    margin: 0 auto; 
    text-align: center; 
    font-size: 12px; 
    border-left: 1px solid #ccc; 
    border-right: 1px solid #ccc; 
} 
.stepper .normal{ 
    color: black; 
} 
.stepper .disabled{ 
    color: #ccc; 
} 