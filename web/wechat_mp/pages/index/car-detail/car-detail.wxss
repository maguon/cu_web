.code {
  margin-top: 0px;
  height: 150px;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background:#efeef4;
}
.codeinfo{
  height: 100px;
  width: 100px;
}
.car{
  height: 40px;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.car-img{
 width: 20px;
 height: 20px;
 margin-left: 15px;
 display: flex;
 align-items: center;
}
.nub{
  margin-left: 15px;
  font-size: 15px;
}
.divLine{
 background: rgb(231, 233, 226);
 height: 5rpx;
 margin: 0px 15px 0px 15px;
}
.divLine01{
 margin: 0px 15px 0px 15px;
 border:1px dashed #e4e5de;
}
.car-msg{
  color: #8c8c8c;
  margin-left: 25px;
  font-size: 12px;

}

.but-list{
  background: #f5f5f8;
  height: 50px;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  position:fixed;
  bottom:0px;
  left:0px;

}
.downlode{
  height: 30px;
  width: 150px;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.downlode-img{
  height: 20px;
  width: 20px;
  margin-top: 5px;

}
.download-text{
  font-size: 12px;
  color: #008fe0;
  margin-left: 5px;
}