<!--index.wxml-->
<block wx:for='{{carList}}' wx:key='u'>
    <view class='carbox'>
      <view class='left'>
        <image class='left-img' src='../../../assets/images/car.png'></image>
          <text class='left-text'>{{item.license_plate}}
            <text  class='left-car'>编号：{{item.id}}</text></text>
      </view>
      <view class='right' bindtap='bindCarDetail' data-index='{{index}}'>
      <view class='center'>
        <image class='center-img' src='../../../assets/images/code.png'></image>
      </view>
      <view>
        <image class='right-img' src='../../../assets/images/arrow.png'></image>
      </view> 
    </view>
    </view>
     <view class="divLine"></view>
 </block>

<view bindtap='relevance' class='relevance'>
<image class='relevance-img' src='../../../assets/images/add-car.png'></image>
<text class='relevance-text'>关联车辆</text>
</view>


